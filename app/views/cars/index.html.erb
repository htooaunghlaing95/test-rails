<h1>All Cars</h1>
		
	<div>
		<table border="2">
			<thead>
				<tr>
					<th>Car Name</th>
					<th>Actions</th>
				</tr>
			</thead>
		
		<tbody>
			<% @cars.each do |car| %>
				<tr>
					<td><%= car.name %></td>
					<td>
						<%= link_to "Show", car_path(car) %>
						<%=	link_to "Edit", edit_car_path(car) %>
						<%= link_to "Delete", car_path(car),
						method: :delete %>
					</td>
				</tr>
			<% end %>
		</tbody>
	</table>
	</div>

	<%= link_to "Create New Car", new_car_path(@car) %> 
	<br>
	<%= link_to "Back" , root_path %>